<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#dd33b5e5" android:endColor="#dd33b5e5" android:angle="270.0" android:centerY="0.75" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ff33b5e5" android:endColor="#ff33b5e5" android:angle="270.0" android:centerY="0.75" />
        </shape>
    </item>
</selector>